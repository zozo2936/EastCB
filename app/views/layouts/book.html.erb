<!DOCTYPE html>
<html>
  <head>
    <title>EastCB</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'shared/frontend/navbar'%>
    <%= render 'shared/flash' if notice%>

    <section class="columns is-fullheight">
      <%= render 'shared/frontend/menu',publishers: Publisher.available,categories: Category.all%>
        <div class="column is-10">
          <%= yield %> 
        </div>
    </section>
  </body>
</html>
